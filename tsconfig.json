{
	"compilerOptions": {
        "module": "esNext",
		"target": "ES6",
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
		"preserveConstEnums":true,
		"sourceMap": true,
		"rootDir": "src",
	},
}